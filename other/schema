CREATE TABLE users (
	OpenID varchar(255) PRIMARY KEY,
	ActiveChannel int
)

CREATE TABLE tag(

	TagID varchar(255) PRIMARY KEY,
	Name  varchar(255),
	Description  varchar(255),
	OwnerID varchar(255) references users(OpenID) NOT NULL,
	OwnerChannel int CHECK(OwnerChannel >= 1 AND OwnerChannel <= 10),
	FinderID varchar(255) references users(OpenID),
	FinderChannel int CHECK(FinderChannel >= 11 AND FinderChannel <=20),
	UNIQUE(OwnerID, OwnerChannel)
)
